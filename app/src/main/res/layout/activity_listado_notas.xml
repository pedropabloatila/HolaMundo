<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.holamudo.datos.ListadoNotasActivity">

    <EditText
        android:id="@+id/editListadoNotas1"
        android:layout_width="394dp"
        android:layout_height="130dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:ems="10"
        android:gravity="start|top"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editListadoNotas2"
        android:layout_width="391dp"
        android:layout_height="148dp"
        android:layout_marginTop="46dp"
        android:ems="10"
        android:gravity="start|top"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="@+id/editListadoNotas1"
        app:layout_constraintStart_toStartOf="@+id/editListadoNotas1"
        app:layout_constraintTop_toBottomOf="@+id/editListadoNotas1" />
</android.support.constraint.ConstraintLayout>